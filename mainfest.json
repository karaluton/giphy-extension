{
    "name": "Giphy",
    "version": 1.0,
    "description": "The most popular gifs from Giphy in your default tab.",
    "author": "Kara Luton",
    "manifest_version": 2,
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "128": "icon.png"
    }
}